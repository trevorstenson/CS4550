server {
        listen 80;
        listen [::]:80;

        root /home/trevor/www/hw02.downwind.xyz;

        index index.html;

        server_name hw02.downwind.xyz www.hw02.downwind.xyz;

        location / {
                # First attempt to serve request as file, then
                # as directory, then fall back to displaying a 404.
                try_files $uri $uri/ =404;
        }
}
